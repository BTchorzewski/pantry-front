{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "useTabs": false,
  "endOfLine": "auto",
  "jsxSingleQuote": true,
  "quoteProps": "as-needed",
  "arrowParens": "always",
  "proseWrap": "always",
  "bracketSpacing": true,
  "jsxBracketSameLine": false
}
